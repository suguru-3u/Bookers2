<div class="container">
  <div class="row">
    <%= render 'layouts/new' %>

    <div class="col-xs-9">
      <h2>Users</h2>
      <table class="table">
          <thead>
            <tr><th>image</th><th>name</th></tr>
          </thead>
          <tbody>
          <% @users.each do |users|%>
          <tr>
            <td><%= attachment_image_tag users,:profile_image, :fill, 50, 50, format: 'jpeg', fallback: "no_image.jpg" %></td>
            <td><%= users.name %></td>
            <td><%= link_to 'Show',user_path(users.id)%></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
